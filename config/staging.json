{
  "environment": "staging",
  "app": {
    "url": "https://staging.visionday.app",
    "port": 80,
    "debug": false,
    "logLevel": "info",
    "cors": {
      "origins": [
        "https://staging.visionday.app",
        "https://staging-api.visionday.app",
        "http://localhost:3000"
      ]
    }
  },
  "database": {
    "ssl": true,
    "poolSize": 15
  },
  "security": {
    "sessionTimeout": 3600,
    "rateLimiting": {
      "windowMs": 900000,
      "maxRequests": 75
    }
  },
  "services": {
    "sentry": {
      "environment": "staging",
      "sampleRate": 0.5
    },
    "monitoring": {
      "enabled": true
    }
  },
  "features": {
    "analytics": true,
    "notifications": true,
    "collaboration": true,
    "advancedReports": true,
    "aiAssistant": true,
    "apiV2": true
  },
  "performance": {
    "caching": {
      "enabled": true,
      "ttl": 3600,
      "maxSize": 536870912
    },
    "compression": {
      "enabled": true,
      "level": 6
    },
    "cdn": {
      "enabled": true,
      "baseUrl": "https://staging-cdn.visionday.app",
      "regions": [
        "us-east-1",
        "eu-west-1"
      ]
    }
  },
  "deployment": {
    "strategy": "blue-green",
    "healthCheck": {
      "timeout": 5000
    },
    "rollback": {
      "threshold": 0.05
    }
  }
}